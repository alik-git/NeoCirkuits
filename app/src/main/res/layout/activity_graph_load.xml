<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_graph_load"
    tools:context="com.circuitstudio2016.circuits.GraphLoadActivity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/activity_graph_load_1"
        android:layout_width="105dp"
        android:layout_height="504dp"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/activity_graph_load_2"
        android:layout_width="105dp"
        android:layout_height="504dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintRight_toLeftOf="@+id/activity_graph_load_3"
        android:layout_marginRight="0dp"
        app:layout_constraintLeft_toRightOf="@+id/activity_graph_load_1"
        android:layout_marginLeft="0dp"
        app:layout_constraintHorizontal_bias="0.5">

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/activity_graph_load_3"
        android:layout_width="105dp"
        android:layout_height="504dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent">

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>
