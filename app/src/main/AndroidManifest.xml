<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.circuitstudio2016.circuits">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Neon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayActivity">
            <intent-filter>
                <action android:name="android.intent.action.PLAY" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HamiltonActivity">
            <intent-filter>
                <action android:name="android.intent.action.HCIRCUIT" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MakeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAKE" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HamiltonTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.HamiltonTest" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoadActivity" />
        <activity android:name=".HCircuitLevelsActivity" />
        <activity android:name=".HamiltonPlayActivity" />
        <activity android:name=".EulerTestActivity" />
        <activity android:name=".EulerPlayActivity" />
        <activity android:name=".LevelsActivity" />
        <activity android:name=".ECircuitLevelsActivity" />
        <activity android:name=".GraphLoadActivity" />
        <activity android:name=".SelectActivity" />
        <activity android:name=".HamiltonLevelsActivity"></activity>
    </application>

</manifest>